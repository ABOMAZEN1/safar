{
    "disabled": [
        "QualifiedNamesOnlyForClassName",
        "RemoveLeadingSlashNamespaces"
    ],
    "paths": [
        "app",
        "routes",
        "tests"
    ],
    "exclude": [
        "database/migrations"
    ],
    "rules": {
        "indent": 4,
        "max_line_length": 120,
        "no_trailing_whitespace": true,
        "newline_at_end_of_file": true
    }
}